{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:5678/*"
        }
      ]
    },
    "opener:default",
    "notification:default",
    "store:default",
    "sql:default",
    "sql:allow-execute",
    "dialog:default",
    "fs:default"
  ]
}
